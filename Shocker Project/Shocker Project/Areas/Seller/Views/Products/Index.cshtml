	<script src="~/lib/vue/vue.global.prod.js"></script>
	<script src="~/lib/axios/axios.min.js"></script>
	<script>
		var baseAddress = "https://localhost:7033";
		var vueApp = {
			data() {
				return {
					filter: '',
					details: '',
					Products: [],
					ProductId: '',
					SellerAccount: '',
					LaunchDate: '',
					ProductName: '',
					ProductCategoryId: '',
					Description: '',
					UnitsInStock: '',
					Sales: '',
					UnitPrice: '',
					Status: '',
					Currency: '',
					productNameOld: '',
					productCategoryIdOld: '',
					descriptionOld: '',
					unitsInStockOld: '',
					salesOld: '',
					unitPriceOld: '',
					statusOld: '',
					currencyOld: ''
				};
			},
			mounted: function () {
				let _this = this;
				_this.getProducts();
			},
			methods: {
				getProducts: function () {
					//alert('getProducts');
					let _this = this;
					axios.post(`${baseAddress}/Seller/Products/getProducts`).then(
						response => {
							_this.Products = response.data.map(x => ({ ...x, Edit: false }));
						}
					);
				},
				filterProducts: function () {
					alert('filter');
					let _this = this;
					var request = {};
					request.ProductId = isNaN(parseInt(_this.filter)) ? -1 : parseInt(_this.filter);
					request.ProductName = request.ProductCategoryId = request.Description = request.UnitsInStock = request.Sales = request.UnitPrices = request.Status = request.Currency;
					axios.get(`${baseAddress}/Seller/Products/Filter`, request).then(response => {
						_this.Products = response.data;
						var ProductList = [];
						for (var i = 0; i < _this.Products.length; i++) {
							var item = _this.Products[i];
							item.Edit = false;
							ProductList.push(item);
						}
						_this.Products = ProductList;
					});
				},
				edit: function (productId) {
					//alert('edit');
					let _this = this;
					_this.Products.forEach(item => {
						if (item.ProductId == productId) {
							item.Edit = true;
							_this.productNameOld = item.ProductName;
							_this.productCategoryIdOld = item.ProductCategoryId;
							_this.descriptionOld = item.Description;
							_this.unitPriceOld = item.UnitPrice;
							_this.salesOld = item.Sales;
							_this.statusOld = item.Status;
							_this.currencyOld = item.Currency;
						}
					});
				},
				update: function (item) {
					alert('update');
					let _this = this;
					var request = {};
					request.ProductName = item.ProductName;
					request.ProductCategoryId = item.ProductCategoryId;
					request.Description = item.Description;
					request.UnitPrice = item.UnitPrice;
					request.Sales = item.Sales;
					request.Status = item.Status;
					request.Currency = item.Currency;
					axios.get(`${baseAddress}/seller/Products/${item.ProductId}`, request).then(response => {
						alert(response.data);
						_this.cancel();
						_this.filterProducts();
						//item.Edit = false;
					});
				},
				cancel: function () {
					alert('cancel');
					let _this = this;
					var ProductList = [];
					for (var i = 0; i < _this.Products.length; i++) {
						var item = _this.Products[i];
						if (item.Edit == true) {
							item.ProductName = _this.productNameOld;
							item.ProductCategoryId = _this.productCategoryIdOld;
							item.Description = _item.descriptionOld;
							item.UnitPrice = _this.unitPriceOld;
							item.Sales = _this.salesOld;
							item.Status = _this.statusOld;
							item.Currency = _this.currencyOld;
						}
						ProductList.push(item);
					}
					_this.Products = ProductList;
				},
				deleteProduct: function (productId) {
					let _this = this;
					var ret = confirm('確定要刪除嗎？');
					if (ret == true) {
						axios.get(`${baseAddress}/Seller/Products/Delete/${item.ProductId}`).then(response => {
							alert(response.data);
							_this.filterProducts();
						});
					}
				},
				showModal: function () {
					alert('show');
					$('#insertModal').modal('show');
				},
				insert: function () {
					alert('insert');
					var _this = this;
					var request = {};
					request.ProductId = 0;
					request.ProductName = _this.ProductName;
					request.ProductCategoryId = _this.ProductCategoryId;
					request.Description = _this.Description;
					request.UnitPrice = _this.UnitPrice;
					request.Sales = _this.Sales;
					request.Ptatus = _this.Status;
					request.Purrency = _this.Currency;
					axios.get(`${baseAddress}/Seller/Products/Create`, request).then(
						response => {
							alert(response.data);
							$('#insertModal').modal('hide');
							_this.filterProducts();
						}
					);
				}
			}
		}
		var app = Vue.createApp(vueApp).mount('#app');
	</script>
